#include <iostream>
using namespace std;
int main()
{
 int arr[3][3], arr2[3][3];
cout<<"Enter MATRIX : ";
for(int i=0; i<3; i++)
{
for(j=0; j<3; j++)
{
cin>>arr[i][j];
}
}
for(int i=0; i<3; i++)
{
for(int j=0; j<3; j++)
{
arr2[i][j]=arr[j][i];
}
}
cout<<"Transpose of your MATRIX is :\n";
for(int i=0; i<3; i++)
{
for(j=0; j<3; j++)
{
cout<<arr2[i][j];
}
cout<<"\n";
}
 return 0;
}
